/*-- About Dengue --*/
"about_dengue_title" = "டெங்கி";
"about_dengue_para1" = "டெங்கி அல்லது  டெங்கி காய்ச்சல்,  ஏடிஸ் கொசுக்கள் மூலம் பரவும் வைரஸ் நோய் ஆகும்.";
"about_dengue_para2" = "டெங்கியை குணப்படுத்த மருந்து அல்லது தடுப்பூசி கண்டுப் பிடிக்கப்படவில்லை. இதை தவிர்க்க ஏடிஸ் கொசுக்களின் இனப்பெருக்கத்தை தடுப்பதே ஒரே சிறந்த வழியாகும்.";

/*-- Dengue Spread --*/
"dengue_spread_title1" = "டெங்கி எப்படி பரவுகிறது?";
"dengue_spread_para1" = "டெங்கி 2 வழிகளில் பரவுகிறது, மனிதனிடமிருந்து கொசுகளுக்கும் மற்றும் கொசுகளிடமிருந்து மனிதனுக்கும் பரவுகிறது.";
"dengue_spread_para2" = "<u>கொசுகளிடமிருந்து மனிதனுக்கு</u>: பாதிக்கப்பட்டப் பெண் ஏடிஸ் கொசுகள் மனிதர்களை கடிக்கும் போது அவர்களுக்கு வைரஸ் கடக்கின்றது.";
"dengue_spread_para3" = "<u>மனிதனிடமிருந்து கொசுகளுக்கு</u>: வைரஸ்யை மனிதர்கள் அவர்களுக்கு உள்ளே காவியும் பெருக்கியும் மேலும் பாதிக்கப்படாத கொசுகள் அவர்களை கடிக்கும் போதும் அவற்றுக்கும் பரவுகின்றன. நோயாளியை கடித்த பின்னர் ஒரு வாரம் கழித்து கொசுகளுகும் தொற்று ஏற்படுகிறது.";

"dengue_spread_title2" = "ஏடிஸ் கொசுக்களை எப்படி கண்டுக்  கொள்வது?";
"dengue_spread_para4" = "நீங்கள் ஏடிஸ் கொசுக்களை அதன் உடலில் காணப்படும் கருப்பு மற்றும் வெள்ளை கோடுகள் மூலம் அடையாளம் காண முடியும்.";
"dengue_spread_para5" = "ஏடிஸ் கொசுக்கள் அதிகாலை அல்லது அந்தி பொழுதில் கடிக்க முனைவதால், அவ்வேளைகளில் கூடுதல் கவனம் தேவை.";
"dengue_spread_para6" = "ஒரு ஏடிஸ் கொசுக்கள் இது போல இருக்கும்.";

"dengue_spread_title3" = "ஆபத்தான இடங்கள்";
"dengue_spread_title4" = "முகப்பு";
"dengue_spread_title5" = "பொது பகுதிகள்";

/*-- Dengue Symptoms --*/
"dengue_symptoms_para1" = "திடிரென்ன அதிகமான காச்சல் (2 முதல் 7 நாட்கள் வரை).";
"dengue_symptoms_para2" = "தலைவலி மற்றும் கண் பின்னால் இருந்து வலி.";
"dengue_symptoms_para3" = "தடிப்புக்கள் வெளிப்படுதல்.";
"dengue_symptoms_para4" = "தசை மற்றும் மூட்டு வலி.";
"dengue_symptoms_para5" = "குமட்டல் மற்றும் வாந்தி.";
"dengue_symptoms_para6" = "மூக்கு அல்லது பல் ஈறுகளில் இரத்தப்போக்கு, கருப்பு-பழுப்பு வாந்தி அல்லது மலம், கடுமையான வயிற்று வலி, உடல் குளிர் அல்லது வெளுப்பு , அமைதியின்மை மற்றும் அயர்வு ஆகியவை மேலும் மற்ற அறிகுறிகள் ஆகும்.";
"dengue_symptoms_para7" = "பெரும்பாலான நோயாளிகள் சுமார் 3 அல்லது 4 நாட்களில் டெங்கி காய்ச்சலில் இருந்து மீளமுடியும், ஆனால் அது தீவிரமானால் மரணத்திற்கு வழிவகுக்கும்.";
"dengue_symptoms_title1" = "டெங்கி அறிகுறிகள்";

/*-- Dengue Treatment --*/
"dengue_treatment_title1" = "சிகிச்சை";
"dengue_treatment_para1" = "ஒரு தகுதியான மருத்துவரிடம் ஆலோசிக்கவும்!";
"dengue_treatment_para2" = "உங்கள் மருத்துவர் பரிந்துரைத்த மருந்துகள்  தவிர வேறு மருந்துகள்  சாப்பிட வேண்டாம். மருந்துகள் தவறாக பாவிக்கப்பட்டால் உட்புற  இரத்த  போக்கு ஏற்படும்.";
"dengue_treatment_para3" = "நிறைய ஓய்வு எடுக்கவும்.";
"dengue_treatment_para4" = "அதிக தண்ணீர் குடிக்கவும்.";
"dengue_treatment_para5" = "சத்தான உணவுகளை சாப்பிடவும்.";
"dengue_treatment_para6" = "பள்ளி, வேலை அல்லது வைரஸ் பரவக்கூடிய வேறு எந்த இடங்களுக்கும் செல்வதை தவிர்க்கவும்.";
"dengue_treatment_title2" = "கொழும்பு நகரில் உள்ள மருத்துவமனைகள்";
"dengue_treatment_para7" = "<a href=\"https://maps.google.lk/maps?q=colombo+hospitals&amp;sll=6.9215305,79.8648533&amp;spn=0.2617848,0.351645&amp;t=m&amp;hq=hospitals&amp;hnear=colombo,+Western+Province\">► வெளிப்புறமாக, வரைபடத்தைத் திறகுக!</a>";

/*-- Dengue Prevention --*/
"dengue_prevention_title1" = "எப்படி தடுக்கலாம்";
"dengue_prevention_para1" = "டெங்கி கொசுக்கள் இனப்பெருகுவதற்கு சுத்தமான தண்ணீர் அவசியம். சுத்தமான தண்ணீர் கொண்ட கொள்கலன்களை விட்டுவிட வேண்டாம். அவைகள் ஏடிஸ் கொசுக்கள் பெருகுவதற்கு சாத்தியமான நிலங்களாக மாற முடியும்.";
"dengue_prevention_para2" = "கொள்கலன்களை  மூடவும் அல்லது பயன்படுத்தாத போது அவற்றை கவிழ்த்து விடவும்.";
"dengue_prevention_para3" = "அடிக்கடி சாடிகளில் உள்ள தண்ணீரை மாற்றவும்.";
"dengue_prevention_para4" = "அடிக்கடி வீட்டு செல்லப்பிராணிகளுக்கு கொடுக்கும் தண்ணீரை மாற்றவும்.";
"dengue_prevention_para5" = "நீங்கள் மீன்கள் வளர்ப்பவர் என்றால், சிறிய கொசுக்கள் உண்ணும் மீன்களை  வளர்க்கவும்.";
"dengue_prevention_para6" = "திறந்த காலியான உணவு கொள்கலங்களை உதார்ணதிக்கு ஐஸ் கிரீம் கப், தேங்காய் ஓடுகள், பொலித்தீன் பைகள் ஆகியவைகளை எறிவதை தவிர்க்க வேண்டும். அவைகள் மழை பெய்யும் போது நீரை தேக்கும்.";
"dengue_prevention_para7" = "வடிகால்களை தடுக்கும் இலைகள் மற்றும் பிற குப்பைகளை அகற்றவும்.";
"dengue_prevention_para8" = "வாய்க்கால்களை சுத்தப்படுத்தி எந்த அடைப்பும் இல்லை என்பதை உறுதி செய்யவும்.";
"dengue_prevention_para9" = "கூரை மேல் உள்ள மற்றும் தோட்டங்களில் உள்ள டயர்களில் மண் நிரப்பி அவைகளில் தண்ணீர் தேங்காமல் தடுக்கவும்.";


//Mo-Buzz information
/*-- About Mo-Buzz --*/
"about_mobuzz_title1" = "Mo-Buzz Dengue*";
"about_mobuzz_para1_1" = "டெங்கி கொசுக்களால் பரவும் தொற்று நோய் ஆகும். இதனால் உலகம் முழுவதும் மில்லியன் கணக்கான மக்கள் பாதிக்கப்பட்டு உள்ளனர் குறிப்பாக சொல்லப்போனால் ஆசியாவில்.";
"about_mobuzz_para1_2" = "இதுவரை டெங்கி ஒழிப்பு பாரம்பரிய உத்திகளை நம்பியிருந்தது. அவை பெரும்பாலும் பிற்போக்கான எதிர்வினையாகவும் மற்றும் காலாவதியான தொடர்பு முறைகளுமாக இருந்துள்ளன.";
"about_mobuzz_para1_3" = "நாம் கையடக்க தொலைபேசி மற்றும் இணைய தளச் சக்தியை ஒரு புதுமையான வழியில் பயன்படுத்தி, மக்கள் மற்றும் பொது சுகாதார அதிகாரிகள் டெங்கியை எதிர்க்க நீண்ட வழியில் உதவ முடியும் என்று நம்புகின்றோம்.";
"about_mobuzz_title2" = "Mo-Buzz Dengue -க்கு வரவேற்கிறோம்!";
"about_mobuzz_para2_1" = "Mo-Buzz Dengue, நீங்கள் டேங்கிலிருந்து உங்களை பாதுகாக்க உதவும் வகையில் வடிவமைக்கப்பட்ட ஓர் அதி உன்னத, ஒருங்கிணைந்த கையடக்க தொலைபேசி மற்றும் இணைய தள மென் ஒருங்கு அடிப்படையில் அமைக்கப்பட்ட ஒன்றாகும்.";
"about_mobuzz_para2_2" = "இந்த அமைப்பு மேம்பட்ட நிலபரப்பு பட வரைவுடன், குழு பங்களிப்பு தொழில்நுட்பம் மற்றும் சுகாதார கல்வி ஆகிய ஆற்றல்கள் அனைத்தும் ஒருங்கிணைந்த ஒரு சாதாரண மற்றும் எளிதாக பயன்படுத்தக்கூடிய முறையில் உருவாக்கப்பட்டது.";
"about_mobuzz_para2_3" = "Mo-Buzz Dengue -யுடன் நீங்கள் பின்வருவனவற்றை இப்போது செயலாற்ற முடியும்	";
"about_mobuzz_para2_4" = "பாதிப்புக்குள்ளான இடங்களின்  வரைபடங்களை (hotspots map) பயன்படுத்தி டெங்கினால் மிகவும் பாதிக்கப்பட்ட பகுதி நகரில் எது என்று உங்களால் கண்காணிக்க முடியும். நீங்கள் இந்த பகுதிகளில் வாழ்பவர், அல்லது  பயணம் செய்பவர் என்றால் நீங்கள் இந்த தகவலை தடுப்பு நடவடிக்கைகளை எடுக்கவும் உங்களை கொசுகலிருந்து பாதுகாக்க பயன்படுத்த முடியும்.";
"about_mobuzz_para2_5" = "டெங்கி கொசுக்கள் இனப்பெருக்கம் பரவும் இடங்கள் பற்றியும் மற்றும் டெங்கி புகார்களை உங்கள் கையடக்க தொலைபேசியின் மென் ஒருங்கில் எளிய படிவத்தில் உள்ள புவியியல் இடத்தின் பதியை பயன்படுத்தி  கொழும்பு  மாநகர  சபைக்கு  அறிவிக்கலாம்.  இது கொழும்பு  மாநகர  சபைக்கு  டெங்கி பரவுவதை உடனுக்குடன் அறியவும் தேவையான எச்சரிக்கை  நடவடிக்கைகளை விரைவில் எடுக்கவும் உதவும்.";
"about_mobuzz_para2_6" = "டெங்கி பற்றி மேலும் அறிய, அது எவ்வாறு பரவுகிறது, எப்படி மக்களை பாதிக்கிறது மற்றும் உங்களை அதிலிருந்து எப்படி பாதுகாப்பது என்பதனை அறிய எங்கள்  வரைகலை அடிப்படையிலான டெங்கி கல்வி தொகுதியை பயன்படுத்தலாம்.";
"about_mobuzz_para3" = "*Mo-Buzz Dengue நன்யாங் தொழில்நுட்ப பல்கலைக்கழகம் (NTU), கொழும்பு மாநகர சபை (CMC), மொபிட்டல் (Mobitel) மற்றும் கொழும்பு பல்கலைக்கழகதின் கணினி பள்ளிக்கிடையே (UCSC) ஓர் கூட்டு திட்டம் ஆகும்.";


