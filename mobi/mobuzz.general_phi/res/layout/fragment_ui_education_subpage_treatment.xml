<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UiEducationSubpageTreatment" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="35dp" >

                <TableRow
                    android:id="@+id/tblr_dengue_treatment_title1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/txt_dengue_treatment_title1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dengue_treatment_title1"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@color/mobuzz_gray" />
                </TableRow>

                <TableRow
                    android:id="@+id/tblr_dengue_treatment_para1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp" >

                    <TextView
                        android:id="@+id/txt_dengue_treatment_para1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dengue_treatment_para1"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@color/mobuzz_gray" />
                </TableRow>

                <TableRow
                    android:id="@+id/tblr_dengue_treatment_para2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp" >

                    <TextView
                        android:id="@+id/txt_dengue_treatment_para2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dengue_treatment_para2"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@color/mobuzz_gray" />
                </TableRow>

                <TableRow
                    android:id="@+id/tblr_dengue_treatment_para3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp" >

                    <TextView
                        android:id="@+id/txt_dengue_treatment_para3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dengue_treatment_para3"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@color/mobuzz_gray" />
                </TableRow>

                <TableRow
                    android:id="@+id/tblr_dengue_treatment_para4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp" >

                    <TextView
                        android:id="@+id/txt_dengue_treatment_para4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dengue_treatment_para4"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@color/mobuzz_gray" />
                </TableRow>

                <TableRow
                    android:id="@+id/tblr_dengue_treatment_para5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp" >

                    <TextView
                        android:id="@+id/txt_dengue_treatment_para5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dengue_treatment_para5"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@color/mobuzz_gray" />
                </TableRow>

                <TableRow
                    android:id="@+id/tblr_dengue_treatment_para6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp" >

                    <TextView
                        android:id="@+id/txt_dengue_treatment_para6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dengue_treatment_para6"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@color/mobuzz_gray" />
                </TableRow>

            </TableLayout>

            <TableRow
                android:id="@+id/tableRow1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp" >

                <TextView
                    android:id="@+id/txt_dengue_treatment_title2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/dengue_treatment_title2"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/mobuzz_gray" />

            </TableRow>

            <WebView
                android:id="@+id/webv_dengue_treatment"
                android:layout_width="match_parent"
                android:layout_height="500dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp" />

            <TextView
                android:id="@+id/txt_link_hospital"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="35dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                android:text="@string/dengue_treatment_para7"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/mobuzz_gray" />

        </LinearLayout>

    </ScrollView>

</RelativeLayout>